module Std.Bit where



export pure (<<) => { Integral n }, n, n, n;
a << b := rose_shiftl__ a b;


export pure (&) => { Integral n }, n, n, n;
a & b := rose_and__ a b;


export pure (|) => { Integral n }, n, n, n;
a | b := rose_or__ a b;


export pure (^) => { Integral n }, n, n, n;
a ^ b := rose_xor__ a b;


export pure (>>) => { Integral n }, n, n, n;
a >> b := rose_shiftr__ a b;
